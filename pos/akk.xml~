	<tag desc="pronoun" name="PRO" regex="^(szu)$" equals="" case="THIRD_SINGULAR_NOMINATIVE" value="he" />
	<tag desc="pronoun" name="PRO" regex="^(szi)$" equals="" case="THIRD_SINGULAR_NOMINATIVE" value="she" />
	<tag desc="pronoun" name="PRO" regex="^(ni-(i-)?nu)$" equals="" case="FIRST_PLURAL_NOMINATIVE" value="we" />
