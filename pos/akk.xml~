	<!-- Relative Pronouns -->
	<tag desc="pronoun" name="PRO" equals="" regex="^(sza-at|sza-a-ti|szut|szat|szu|sza|szi)$" cunei="𒐼" case="PRO" value="who" />
	<tag desc="pronoun" name="PRO" equals="" regex="^(un-du)$" cunei="" case="PRO" value="when/then" />
	<!-- Demonstrative Pronouns -->
