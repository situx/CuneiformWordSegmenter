    <colors>
	<tagcolor tag="DEFAULT" desc="default" matchorder="13" color="#C0C0C0"/>
        <tagcolor tag="CARD" desc="number" matchorder="1" color="#FFC0CB"/>
	<tagcolor tag="PREP" desc="preposition" matchorder="2" color="#00FFFF"/>
	<tagcolor tag="DET" desc="determinative" matchorder="3" color="#FFA500"/>
	<tagcolor tag="IP" desc="indefinitepronoun" matchorder="4" color="#A5A959"/>
	<tagcolor tag="CONJ" desc="conjunction" matchorder="5" color="#FF0000"/>
	<tagcolor tag="PRE" desc="precative" matchorder="6" color="#CFF1D1"/>
	<tagcolor tag="PRO" desc="pronoun" matchorder="7" color="#0000FF"/>
	<tagcolor tag="PART" desc="particle" matchorder="8" color="#9370DB"/>
	<tagcolor tag="NA" desc="nounoradj" matchorder="9" color="#FFFF00"/>
	<tagcolor tag="VV" desc="verb" matchorder="10" color="#00FF00"/>
