        <tag desc="preposition" name="PREP" equals="" regex="^(it-ti)$" case="PREP" value="with" />
        <tag desc="preposition" name="PREP" equals="" regex="^(ki-ma)$" case="PREP" value="like,as" />
	<tag desc="preposition" name="PREP" equals="" regex="^(ki-a-am)$" case="PREP" value="as follows" />
	<tag desc="preposition" name="PREP" equals="" regex="^(ki-mu)$" cunei="𒆠𒀀" case="PREP" value="like,as" />
        <tag desc="preposition" name="PREP" equals="" regex="^(ki-i-ma)$" cunei="𒆠𒐊𒈠" case="PREP" value="like,as" />
