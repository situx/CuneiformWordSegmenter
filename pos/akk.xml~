	<!-- Prepositions -->
        <tag desc="preposition" name="PREP" equals="" regex="^(a-na)$" cunei="𒀀𒉌" case="PREP" value="to" />
        <tag desc="preposition" name="PREP" equals="" regex="^(a-di)$" case="PREP" value="until" />
        <tag desc="preposition" name="PREP" equals="" regex="^(e-li)$" case="PREP" value="on" />
