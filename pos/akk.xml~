	<!-- Independent Personal Pronouns Nominative -->
	<tag desc="pronoun" name="PRO" regex="^a-na-ku(m|-i|-ia|-ni|-ma|-nim)?$" equals="" case="FIRST_SINGULAR_NOMINATIVE" value="I" />
	<tag desc="pronoun" name="PRO" equals="" regex="^(at-ta(-ka|-kum|ku-um))$" case="SECOND_SINGULAR_NOMINATIVE" value="you" />
