    </colors>
    <groupconfigs>
      <groupconfig tag="VV" desc="verb">
	<!-- (la/li/le/lu for past tense or ni for plural) (Vocal before first root) (Vocal after first root) (ta-,te- for Perfect) -->
	<group desc="verb" name="Case:  First Person Plural" regex="^(ni|nu)-$" group="2" case="declination" value="FIRST_PLURAL"/>
	<group desc="verb" name="Tense: Past Tense" regex="^(li|la|le|lu)-$" group="2" case="tense" value="PAST" />
	<group desc="verb" name="Case:  Third Person Singular" regex="^([^n]?i|u[0-9]?)(-)?$" group="3" case="declination" value="THIRD_SINGULAR"/>
	<group desc="verb" name="Case:  First Person Singular" regex="^(a|u)(-)?$" group="3" case="declination" value="FIRST_SINGULAR" />
	<group desc="verb" name="Case:  Second Person Singular" regex="^(ta|tu)-$" group="3" case="declination" value="SECOND_SINGULAR"/>
	<group desc="verb" name="N-Stem" regex="([bcdfghjklmnpqrstvwxyz])-\1" group="4"/>
	<group desc="verb" name="Tense: Past Tense" regex="t-t" group="4" case="tense" value="PAST" />
	<group desc="verb" name="Tense: Perfect" regex="(ta|te)-" group="6" case="tense" value="PERFECT" />
	<group desc="verb" name="Tense: Present/Future" regex="((.)[^-](.)|(.)-(.))" group="8" case="tense" value="PRESENT"/>
	<group desc="verb" name="Case:  Third Person Plural" regex="^(u2)$" group="10" case="declination" value="THIRD_PLURAL"/>
	<group desc="verb" name="Case:  Second Person Plural" regex="^(a2)$" group="10" case="declination" value="SECOND_PLURAL" />
	<group desc="verb" name="Case:  Second Person Singular Female" regex="^(-i2)$" group="10" case="declination" value="SECOND_SINGULAR_FEMALE"/>
      </groupconfig>
    </groupconfigs>
    <tags>
