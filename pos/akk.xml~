	<tag desc="pronoun" name="PRO" regex="^(szi-na)$" equals="" case="THIRD_PLURAL_NOMINATIVE_FEMALE" value="they" />
	<!-- Independent Personal Pronouns Oblique -->
	<tag desc="pronoun" name="PRO" equals="ia-ti" regex="" case="FIRST_SINGULAR_GENAKK" value="my" />	
	<tag desc="pronoun" name="PRO" equals="" regex="^(ka-ti|ka-ta)$" case="SECOND_SINGULAR_GENAKK" value="your" />
	<tag desc="pronoun" name="PRO" equals="" regex="sz[iau]-a-t[ui]" case="THIRD_SINGULAR_GENAKK" value="he/she/it" />
	<tag desc="pronoun" name="PRO" equals="ni-a-ti" regex="" case="FIRST_PLURAL_GENAKK" value="we" />
	<tag desc="pronoun" name="PRO" equals="" regex="(ku-nu-ti|ki-na-ti)" case="SECOND_PLURAL_GENAKK" value="you" />	
	<tag desc="pronoun" name="PRO" equals="" regex="^(szu-nu-ti)$" case="SECOND_PLURAL_GENAKK_MALE" value="they" />
	<tag desc="pronoun" name="PRO" equals="" regex="^(szi-na-ti)$" case="SECOND_PLURAL_GENAKK_FEMALE" value="they" />
	<!-- Independent Personal Pronouns Dativ -->
	<!-- Suffixed Personal Pronouns -->
        <tag desc="pronoun" name="PRO" equals="" regex="^ia-szi[m]?$" case="FIRST_SINGULAR_DATIVE" value="me" />
	<tag desc="pronoun" name="PRO" equals="" regex="^ka-szi[m]?$" case="SECOND_SINGULAR_DATIVE" value="you" />	
	<tag desc="pronoun" name="PRO" equals="" regex="^(szu-a-szi[m]?|sza-sz[ui][m]?)$" case="THIRD_SINGULAR_DATIVE" value="he/she/it" />	
	<tag desc="pronoun" name="PRO" equals="" regex="^(ni-a-szi[m]?)$" case="FIRST_PLURAL_DATIVE" value="we" />
